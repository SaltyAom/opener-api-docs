ackground-color: #f9fbff;\r\n}\r\n\r\n::selection, ::-webkit-selection { background-color:rgba(0,123,255,0.25); }\r\n\r\n* {\r\n    font-family: -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, \"Helvetica Neue\", \"Noto Sans\", sans-serif;\r\n    box-sizing: border-box;\r\n    font-weight: normal;\r\n}\r\n\r\nhtml { scroll-behavior: smooth; }\r\n\r\nh1,h2,h3,h4,h5,h6,div,span { cursor:default; }\r\n\r\n::-webkit-scrollbar {\r\n    width:7px;\r\n    height: 7px;\r\n    background-color: transparent;\r\n}\r\n\r\n::-webkit-scrollbar-thumb {\r\n    background-color:var(--primary);\r\n    border-radius: 4px;\r\n}\r\n\r\nsection {\r\n    width:700px;\r\n    margin:15px auto;\r\n    padding:15px 5%;\r\n}\r\n\r\n#banner {\r\n    width:100%;\r\n    height:100vh;\r\n    display:flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    align-items: center;\r\n    text-decoration: none;\r\n    background-image: radial-gradient(circle, var(--primary), var(--primary) 2px, #FFF 2px, #FFF);\r\n    background-size: 36px;\r\n    animation: moving-dot 3s linear 0s infinite;\r\n}\r\n\r\n@keyframes moving-dot {\r\n    to { \r\n        background-position: 36px 0; \r\n    }\r\n}\r\n\r\n#banner-title {\r\n    font-weight: 300;\r\n    text-size-adjust: 100%;\r\n    line-height: 1.2;\r\n    font-size: 5rem;\r\n    color:var(--primary);\r\n    margin:0 0 30px 0;\r\n    cursor:pointer;\r\n}\r\n\r\n#banner-detail {\r\n    font-size: 16px;\r\n    color:var(--less);\r\n}\r\n\r\nsection > h1 { \r\n    font-size: 3rem;\r\n    border-bottom: 1px solid var(--less);\r\n    padding: 15px; \r\n}\r\n\r\nsection > h2 { \r\n    font-size: 1.75rem;\r\n    padding: 8px; \r\n}\r\n\r\nsection > p { \r\n    font-size: 1.15rem; \r\n    color:var(--text); \r\n    line-height: 1.5; \r\n}\r\n\r\nsection > br { \r\n    line-height: 3; \r\n}\r\n\r\nsection > blockquote { \r\n    font-size: 1.5rem;\r\n    padding: 30px;\r\n    color:var(--primary); \r\n    margin:0; \r\n}\r\n\r\nsection > code {\r\n    appearance: none;\r\n    -webkit-appearance: none;\r\n    border:0;\r\n    resize:none;\r\n    background-color: var(--code);\r\n    font-family: Consolas, monospace , Roboto, Helvatic Neue;\r\n    width:100%;\r\n    padding:15px 20px;\r\n    margin-bottom:40px;\r\n    display:block;\r\n    font-size: 1.15rem;\r\n    border-bottom-left-radius: 5px;\r\n    border-bottom-right-radius: 5px;\r\n    white-space: pre-line;\r\n    word-break: break-word;\r\n}\r\n\r\n.code-heading {\r\n    display:flex;\r\n    flex-direction: row;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    border-top-left-radius: 5px;\r\n    border-top-right-radius: 5px;\r\n    margin-top:10px;\r\n}\r\n\r\n.code-heading-button-left, .code-heading-button-right, .code-heading-copy {\r\n    appearance: none;\r\n    -webkit-appearance: none;\r\n    background-color: var(--code);\r\n    border:0;\r\n    font-size: 12px;\r\n    padding: 4px 10px;\r\n    cursor: pointer;\r\n}\r\n\r\n.code-heading-button-left { border-top-right-radius: 0; }\r\n.code-heading-button-right { border-top-left-radius: 0; }\r\n\r\n.code-heading-copy {\r\n    border-top-left-radius: 5px;\r\n    border-top-right-radius: 5px;\r\n    margin-top:10px;\r\n}\r\n\r\n.code-heading-type {\r\n    display:flex;\r\n    flex-direction: row;\r\n}\r\n\r\n.code-heading-button-left.active, .code-heading-button-right.active {\r\n    background-color:var(--code-active);\r\n}\r\n\r\n.text-primary { color:var(--primary); }\r\n.text-less { color:var(--less); }\r\n\r\n.code-text {\r\n    background-color: var(--code);\r\n    padding: 3px 8px;\r\n    color:var(--text);\r\n    font-size: inherit;\r\n    border-radius: 3px;\r\n}\r\n\r\n.code-link {\r\n    background-color: var(--code);\r\n    padding: 3px 8px;\r\n    color:var(--primary);\r\n    font-size: inherit;\r\n    border-radius: 3px;\r\n}\r\n\r\n@media screen and (max-width: 767.9px){\r\n    section {\r\n        width:100% !important;\r\n        padding-left:5% !important;\r\n        padding-right: 5% !important;\r\n    }\r\n    #banner-title {\r\n        font-size: 4.25rem;\r\n    }\r\n}"},"lineCount":190}},"error":null,"hash":"aabe3c3f5497453f3651f7c4a5dd9f17","cacheData":{"env":{}}}